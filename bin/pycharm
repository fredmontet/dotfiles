#!/bin/sh
#Generated by JetBrains Toolbox 1.19.7784 at 2021-01-25T23:25:30.471603

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/frederic.montet/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-P/ch-0/203.6682.179/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"