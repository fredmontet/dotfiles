#!/bin/sh
#Generated by JetBrains Toolbox 1.19.7784 at 2021-01-11T08:42:00.314835

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/frederic.montet/Library/Application Support/JetBrains/Toolbox/apps/WebStorm/ch-0/202.7319.49/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${ideargs[@]}"